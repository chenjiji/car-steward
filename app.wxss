page {
  height: 100%;
  overflow-y: hidden;
  font-size: 24rpx;
  color: #333;
}

.full {
  width: 100%;
  height: 100%;
}

.flex {
  display: flex;
}

.container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.img-box {
  position: relative;
}

.img-box::after {
  display: block;
  content: '';
  padding-top: 100%;
}

.img-box image {
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  position: absolute;
  border-radius: 10rpx;
}
