@import '../../components/common/star.wxss';

.operation-panel {
  position: absolute;
  top: 0;
  left: 0;
  background-color: #fff;
  border-top: 2rpx solid #f90;
  border-bottom: 2rpx solid #f90;
  white-space: nowrap;
  width: 100%;
  height: 77rpx;
}

.operation-item {
  display: inline-block;
  width: 140rpx;
  margin: 12rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 32rpx;
  color: #f90;
  border-radius: 12rpx;
  border: 2rpx solid #f90;
}

.operation-item.active {
  background-color: #f90;
  color: #fff;
}

.list-panel {
  padding-top: 90rpx;
}

.item-panel {
  display: flex;
  padding: 0 30rpx 8rpx 10rpx;
}

.shop-logo {
  width: 220rpx;
  margin-right: 20rpx;
}

.shop-info {
  flex: 1;
}

.shop-name {
  margin-top: 16rpx;
  height: 64rpx;
  line-height: 32rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -webkit-box-pack: center;
  font-size: 28rpx;
  word-break: break-all;
}

.shop-address {
  color: #666;
  height: 56rpx;
  line-height: 28rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -webkit-box-pack: center;
  font-size: 24rpx;
  word-break: break-all;
}

.comment-panel {
  display: flex;
  flex-basis: 100%;
  justify-content: space-between;
  align-items: center;
}

.comment-panel > text {
  text-align: right;
  flex: 1;
  color: #999;
}

.service-panel {
  display: flex;
  flex-basis: 100%;
  justify-content: space-between;
  align-items: center;
}

.service-panel > text {
  text-align: right;
  flex: 1;
  color: #999;
}

.help-panel {
  display: flex;
  align-items: center;
  height: 52rpx;
  width: 176rpx;
}

.help-icon {
  width: 36rpx;
  height: 36rpx;
  margin-right: 8rpx;
}

.help-icon.type0 {
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 36rpx;
}

.help-icon.type1 {
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 44rpx;
}

.help-icon.type2 {
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 40rpx;
}

.help-icon.type3 {
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 48rpx;
}
